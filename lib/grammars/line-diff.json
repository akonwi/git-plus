{
	"fileTypes": [
		"diff"
	],
	"name": "Line Diff",
	"patterns": [{
		"captures": {
			"1": {
				"name": "unimportant"
			},
			"2": {
				"name": "info"
			},
			"3": {
				"name": "insertion"
			},
			"4": {
				"name": "deletion"
			}
		},
		"match": "(\\|)\\s*(\\d+)\\s*(\\+*)(-*)$"
	}, {
		"captures": {
			"1": {
				"name": "info"
			},
			"2": {
				"name": "info"
			},
			"3": {
				"name": "insertion"
			},
			"4": {
				"name": "info"
			},
			"5": {
				"name": "deletion"
			}
		},
		"match": "(?i)(\\d+) files changed, (\\d+) insertions\\((\\+)\\), (\\d+) deletions\\((-)\\)"
	}, {
		"begin": "(?i)^diff --git a/",
		"end": "(?i)index .+$",
		"name": "unimportant"
	}, {
		"begin": "^(---) (a?/)",
		"beginCaptures": {
			"1": {
				"name": "deletion"
			},
			"2": {
				"name": "unimportant"
			}
		},
		"end": "$",
		"contentName": "meta.diff.info.header.from-file"
	}, {
		"begin": "^(\\+\\+\\+) (b?/)",
		"beginCaptures": {
			"1": {
				"name": "insertion"
			},
			"2": {
				"name": "unimportant"
			}
		},
		"end": "$",
		"contentName": "meta.diff.info.header.to-file"
	}, {
		"captures": {
			"1": {
				"name": "unimportant"
			},
			"2": {
				"name": "unimportant"
			}
		},
		"match": "^(@@)\\s*.*\\s*(@@)",
		"name": "info"
	}, {
		"captures": {
			"3": {
				"name": "unimportant"
			},
			"4": {
				"name": "markup.removed.diff"
			},
			"6": {
				"name": "unimportant"
			},
			"7": {
				"name": "markup.removed.diff"
			}
		},
		"match": "^(((<)( .*))|((-)(.*)))$"
	}, {
		"captures": {
			"3": {
				"name": "unimportant"
			},
			"4": {
				"name": "markup.added.diff"
			},
			"6": {
				"name": "unimportant"
			},
			"7": {
				"name": "markup.added.diff"
			}
		},
		"match": "^(((>)( .*))|((\\+)(.*)))$"
	}],
	"scopeName": "source.diff"
}