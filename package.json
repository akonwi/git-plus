{
  "name": "git-plus",
  "main": "./lib/git-plus",
  "version": "5.14.0",
  "description": "Do git things without the terminal",
  "keywords": [
    "git"
  ],
  "activationCommands": {
    "atom-workspace": [
      "git-plus:menu",
      "git-plus:add",
      "git-plus:add-all",
      "git-plus:add-all-and-commit",
      "git-plus:add-all-commit-and-push",
      "git-plus:add-and-commit",
      "git-plus:delete-local-branch",
      "git-plus:delete-remote-branch",
      "git-plus:diff-all",
      "git-plus:diff",
      "git-plus:difftool",
      "git-plus:log",
      "git-plus:log-current-file",
      "git-plus:status",
      "git-plus:push",
      "git-plus:pull",
      "git-plus:pull-using-rebase",
      "git-plus:remove",
      "git-plus:remove-current-file",
      "git-plus:checkout",
      "git-plus:checkout-remote",
      "git-plus:checkout-current-file",
      "git-plus:checkout-all-files",
      "git-plus:cherry-pick",
      "git-plus:commit",
      "git-plus:commit-all",
      "git-plus:commit-amend",
      "git-plus:fetch",
      "git-plus:new-branch",
      "git-plus:reset-head",
      "git-plus:show",
      "git-plus:stage-files",
      "git-plus:stage-hunk",
      "git-plus:stash-save",
      "git-plus:stash-save-message",
      "git-plus:stash-pop",
      "git-plus:stash-keep",
      "git-plus:stash-drop",
      "git-plus:tags",
      "git-plus:unstage-files",
      "git-plus:init",
      "git-plus:run",
      "git-plus:merge",
      "git-plus:merge-remote",
      "git-plus:rebase"
    ]
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "repository": "https://github.com/akonwi/git-plus",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "fs-plus": "^2.2.0",
    "fuzzaldrin": "^1.2.0",
    "underscore-plus": "1.x",
    "atom-space-pen-views": "^2.0.3",
    "flavored-path": "0.0.8",
    "atom-notify": "^1.1.0"
  }
}
