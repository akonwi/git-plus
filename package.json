{
  "name": "git-plus",
  "main": "./lib/git-plus",
  "version": "7.10.0",
  "description": "Do git things without the terminal",
  "keywords": [
    "git"
  ],
  "providedServices": {
    "git-plus": {
      "description": "Execute git commands",
      "versions": {
        "1.0.0": "provideService"
      }
    }
  },
  "scripts": {
    "test": "eslint --fix \"lib/**/*.js\" \"spec/**/*.js\" && apm test"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "autosave": {
      "versions": {
        "1.0.0": "consumeAutosave"
      }
    }
  },
  "repository": "https://github.com/akonwi/git-plus",
  "license": "MIT",
  "engines": {
    "atom": ">= 1.17"
  },
  "dependencies": {
    "ansi-to-html": "^0.4.1",
    "atom-notify": "^1.1.0",
    "atom-space-pen-views": "^2.0.3",
    "fs-plus": "^2.2.0",
    "fuzzaldrin": "^1.2.0",
    "underscore-plus": "^1.6.6"
  },
  "devDependencies": {
    "lint": "^1.1.2",
    "quibble": "^0.3.0"
  }
}
